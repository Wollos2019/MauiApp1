<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="New Page"
        ContentTemplate="{DataTemplate local:NewPage1}"
        Route="NewPage" />

    <ShellContent
        Title="Exercise Page2"
        ContentTemplate="{DataTemplate local:NewPage2}"
        Route="NewPage2" />

    <ShellContent
        Title="Exercise Page3"
        ContentTemplate="{DataTemplate local:NewPage3}"
        Route="NewPage3" />

    <ShellContent
        Title="Exercise Page4"
        ContentTemplate="{DataTemplate local:NewPage4}"
        Route="NewPage4" />

</Shell>
